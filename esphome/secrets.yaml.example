
# Copy to secrets.yaml and fill your values
wifi_ssid: "YOUR_WIFI_SSID"
wifi_password: "YOUR_WIFI_PASSWORD"

# ESPHome API encryption key: `esphome config wizard` can generate one
api_encryption_key: "REPLACE_WITH_YOUR_API_KEY"

# OTA password (optional)
ota_password: "REPLACE_WITH_OTA_PASSWORD"

# Access point for first-time setup
ap_password: "CHANGE_ME_AP_PASSWORD"
